generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  email String @unique
  password String
  cretedAt DateTime @default(now())
}

model Session {
  id String @id
  sid String @unique
  data String @db.Text
  expiresAt DateTime  
}
